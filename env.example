# OpenWebUI Konfiguration
WEBUI_SECRET_KEY=your_secure_secret_key_here
WEBUI_NAME=Company AI Hub

# IONOS AI API Konfiguration
IONOS_TOKEN=your_ionos_api_token_here
IONOS_BASE_URL=https://api.ionos.com/ai/v1

# IONOS PostgreSQL Datenbank Konfiguration
IONOS_DB_HOST=your_ionos_db_host.ionos.com
IONOS_DB_PORT=5432
IONOS_DB_NAME=openwebui
IONOS_DB_USER=your_db_username
IONOS_DB_PASSWORD=your_secure_db_password

# IONOS Redis In-Memory DB Konfiguration
IONOS_REDIS_HOST=your_ionos_redis_host.ionos.com
IONOS_REDIS_PORT=6379
IONOS_REDIS_USER=your_redis_username
IONOS_REDIS_PASSWORD=your_secure_redis_password

# Optional: LDAP Konfiguration (auskommentiert)
#LDAP_BIND_PASSWORD=your_ldap_password
